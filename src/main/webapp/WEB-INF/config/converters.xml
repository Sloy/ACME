<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:util="http://www.springframework.org/schema/util"
	xsi:schemaLocation="
		http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.2.xsd		
		http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-3.2.xsd		
	">

	<util:list id="converters">
		<bean id="stringToCertification" class="com.acme.converters.StringToCertification" />
		<bean id="stringToCountry" class="com.acme.converters.StringToCountry" />
		<bean id="stringToState" class="com.acme.converters.StringToState" />
		<bean id="stringToCity" class="com.acme.converters.StringToCity" />
		<bean id="stringToUser" class="com.acme.converters.StringToUser" />
		<bean id="stringToDate" class="com.acme.converters.StringToDate" />
		<bean id="stringToFamilyProfessional" class="com.acme.converters.StringToFamilyProfessional" />
	</util:list>

</beans>